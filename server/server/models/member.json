{
  "name": "member",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true,
    "allowEternalTokens": true
  },
  "excludeBaseProperties": [
    "email",
    "emailVerified",
    "realm"
  ],
  "properties": {
    "groups": {
      "type": [
        "string"
      ],
      "default": []
    },
    "newUser": {
      "type": "boolean"
    },
    "technicalUser": {
      "type": "boolean"
    },
    "password": {
      "type": "string",
      "required": false
    },
    "type": {
      "type": "string",
      "required": true,
      "default": "local"
    },
    "display": {
      "type": "string",
      "required": false
    },
    "dn": {
      "type": "string",
      "required": false
    },
    "blocked": {
      "type": "boolean",
      "required": false,
      "default": false
    }
  },
  "validations": [],
  "relations": {
    "accessTokens": {
      "type": "hasMany",
      "model": "AccessToken",
      "foreignKey": "userId",
      "options": {
        "disableInclude": true
      }
    },
    "identities": {
      "type": "hasMany",
      "model": "userIdentity",
      "foreignKey": "userId"
    },
    "credentials": {
      "type": "hasMany",
      "model": "userCredential",
      "foreignKey": "userId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    },
    {
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "getUserRoles"
    },
    {
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "getUserConstantVariablePermission"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "groupSolver",
      "permission": "ALLOW"
    },
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "loginUser"
    },
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "isNew"
    },
    {
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "changeUserPassword"
    },
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "getUserDetails"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    }
  ],
  "methods": {}
}
