{
  "name": "tower-ui",
  "version": "1.17.0",
  "private": true,
  "engines": {
    "node": "16.0.0"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "rm -rf ../server/client/* && vue-cli-service build --mode production --dest ../server/client/ui && cp ../server/redirect/index.html ../server/client/index.html",
    "testui": "cypress open",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@fontsource/roboto": "^4.3.0",
    "@mdi/js": "^7.0.96",
    "animejs": "^3.2.1",
    "axios": "^0.21.1",
    "brace": "^0.11.1",
    "core-js": "^3.6.5",
    "fast-sort": "^2.2.0",
    "file-saver": "^2.0.5",
    "filter-data": "^0.1.3",
    "highlight.js": "^9.18.1",
    "prism-core": "^2.0.2",
    "prismjs": "^1.21.0",
    "splitpanes": "2.4.1",
    "vue": "^2.6.11",
    "vuejs-ace-editor": "^1.0.1",
    "vue-animejs": "^2.1.1",
    "vue-cookie": "^1.1.4",
    "vue-custom-scrollbar": "^1.2.0",
    "vue-multipane": "^0.9.5",
    "vue-prism-editor": "^1.2.2",
    "vue-router": "^3.4.9",
    "vue-virtual-scroller": "^1.0.10",
    "vuedraggable": "^2.24.0",
    "vuelidate": "^0.7.5",
    "vuetify": "^2.5.7",
    "vuex": "^3.1.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.4.6",
    "@vue/cli-plugin-eslint": "^3.1.1",
    "@vue/cli-plugin-pwa": "^4.4.6",
    "@vue/cli-service": "^4.5.13",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "cypress": "^5.1.0",
    "eslint": "^6.7.2",
    "eslint-config-vuetify": "^0.2.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "lint-staged": "^11.0.0",
    "prettier": "^1.19.1",
    "sass": "^1.26.3",
    "sass-loader": "^9.0.3",
    "vue-cli-plugin-axios": "0.0.4",
    "vue-cli-plugin-vuetify": "^2.0.7",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.4.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/prettier"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "lint-staged": {
    "*.{js,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
